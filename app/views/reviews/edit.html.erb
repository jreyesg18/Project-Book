<div class=" col-md-6 col-md-offset-3">
  <div class="edit-review-form">

    <h1>Modificacion del analisis</h1>

     <%= simple_form_for([@book, @review]) do |f| %>
         <div id="rating-form">
           <label>Rating</label>
         </div>
         <%= f.input :comentario %>
         <%= f.button :submit, :class => "btn-custom2"%>
     <% end %>
  </div>
</div>

 <script>
   $('#rating-form').raty({
     path: '/assets/',
     scoreName: 'review[rating]'
   });
 </script>